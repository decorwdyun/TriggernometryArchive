<Trigger Enabled="true" Name="O1.  连线组" Id="faf654aa-adea-45d4-88a0-bf39826cb848" RegularExpression="^U7a1_四线_线:my线:(?&lt;idx&gt;[1-4]):is雷:(?&lt;is雷&gt;[01])">
