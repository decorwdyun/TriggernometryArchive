<Trigger Enabled="true" Sequential="True" Name="0. 注册 + 自动开关 （代码注释）" Id="311c3309-7dc5-45af-bb14-982fbd552e57" RegularExpression="^.{15}\S+ 01:" Description="接受：永久字典：HoneyBeeRanking&#xD;&#xA;- channel: 发消息的频道，不存在时为默认值 &quot;p&quot;&#xD;&#xA;&#xD;&#xA;接受：永久表格：HoneyBeeRanking&#xD;&#xA;- hexId: row.Values[0]&#xD;&#xA;- name:  row.Values[1]&#xD;&#xA;- times: row.Values[2]&#xD;&#xA;- lastUpdated: row.Values[3]&#xD;&#xA;&#xD;&#xA;具名回调：HoneyBeeRanking&#xD;&#xA;参数：&#xD;&#xA;- check：&#xD;&#xA;  根据当前小队成员，输出当前是否应该启用：变量 tmp_on = 0 或 1&#xD;&#xA;- ranking：&#xD;&#xA;  生成当前排行并发送&#xD;&#xA;- recent：&#xD;&#xA;  生成最近一批上舰的名字列表并发送">
<Actions>
<Action OrderNumber="1" ActionType="Placeholder" Asynchronous="False" Description="详见触发器描述" DescriptionOverride="True" />
<Action OrderNumber="2" ExecScriptExpression="using System;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Linq;&#xD;&#xA;using Triggernometry;&#xD;&#xA;using Triggernometry.Variables;&#xD;&#xA;using static Triggernometry.PluginBridges.BridgeFFXIV;&#xD;&#xA;using static Triggernometry.Interpreter.StaticHelpers;&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;RealPlugin.plug.RegisterNamedCallback(&#xD;&#xA;    &quot;HoneyBeeRanking&quot;,&#xD;&#xA;    new Action&lt;object, string&gt;(HoneyBeeRanking.Callback),&#xD;&#xA;    null,&#xD;&#xA;    registrant: &quot;蜂蜂小甜心 大航海排行 by 阿洛&quot;&#xD;&#xA;);&#xD;&#xA;&#xD;&#xA;public class HoneyBeeRanking&#xD;&#xA;{&#xD;&#xA;    string channel; // 发消息的频道&#xD;&#xA;    bool isSimp; // 使用简体中文（国服）&#xD;&#xA;    Dictionary&lt;string, VariableDictionary&gt; currentPlayers;&#xD;&#xA;&#xD;&#xA;    List&lt;Player&gt; Ranking;&#xD;&#xA;    List&lt;string&gt; RecentHexIds;&#xD;&#xA;&#xD;&#xA;    public HoneyBeeRanking()&#xD;&#xA;    {&#xD;&#xA;        isSimp = !GetMyself().GetValue(&quot;name&quot;).ToString().Contains(&quot; &quot;);&#xD;&#xA;        channel = GetDictVariable(true, &quot;HoneyBeeRanking&quot;)?.GetValue(&quot;channel&quot;)?.ToString() ?? &quot;&quot;;&#xD;&#xA;        if (string.IsNullOrWhiteSpace(channel)) &#xD;&#xA;            channel = &quot;p&quot;;&#xD;&#xA;        currentPlayers = GetAllEntities()&#xD;&#xA;            .Where(vd =&gt; vd.GetValue(&quot;id&quot;).ToString().StartsWith(&quot;10&quot;))&#xD;&#xA;            .ToDictionary(vd =&gt; vd.GetValue(&quot;id&quot;).ToString(), vd =&gt; vd);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    public static void Callback(object _, string arg)&#xD;&#xA;    { &#xD;&#xA;        var instance = new HoneyBeeRanking();&#xD;&#xA;        switch (arg.ToLower().Trim())&#xD;&#xA;        {&#xD;&#xA;            case &quot;check&quot;:       // 当前队伍中有超过三个已记录的人则开启（输出变量 tmp_on）&#xD;&#xA;                var recordedIds = new HashSet&lt;string&gt;(instance.ParseRecordedPlayersFromTable().Select(p =&gt; p.HexId));&#xD;&#xA;                var count = instance.currentPlayers.Keys.Count(p =&gt; recordedIds.Contains(p));&#xD;&#xA;                SetScalarVariable(false, &quot;tmp_on&quot;, count &gt;= 3 ? &quot;1&quot; : &quot;0&quot;);&#xD;&#xA;                break;&#xD;&#xA;            case &quot;ranking&quot;:     // 生成当前排行并发送&#xD;&#xA;                instance.OutputRanking();       break;&#xD;&#xA;            case &quot;recent&quot;:      // 生成最近一批上舰的名字列表并发送&#xD;&#xA;                instance.OutputRecentNames();   break;&#xD;&#xA;            default: throw new Exception(&quot;HoneyBeeRanking：不支持的参数&quot;);&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    /// &lt;summary&gt; 生成最近一批上舰的名字列表并发送。 &lt;/summary&gt;&#xD;&#xA;    void OutputRecentNames()&#xD;&#xA;    {&#xD;&#xA;        RecentHexIds = GetListVariable(false, &quot;HoneyBeeRecent&quot;).Values.Select(v =&gt; v.ToString()).ToList();&#xD;&#xA;        SetListVariable(false, &quot;HoneyBeeRecent&quot;, null);&#xD;&#xA;&#xD;&#xA;        // /p &lt;se.6&gt;&#xD;&#xA;        // ━━━━━━━━━━━━━━━━━&#xD;&#xA;        // ♥ 蜂蜂小甜心 甜到你心中～ ♥&#xD;&#xA;        // ♥ 歡迎兩位新艦長上艦～ ♥&#xD;&#xA;        // ☆ Aaaaa Bbbbb ☆&#xD;&#xA;        // ☆ 阿阿阿 ☆&#xD;&#xA;&#xD;&#xA;        string cnt = &quot;&quot;;&#xD;&#xA;        if (RecentHexIds.Count &gt; 1)&#xD;&#xA;        {&#xD;&#xA;            cnt = (isSimp ? &quot;零一两三四五六七八&quot; : &quot;零一兩三四五六七八&quot;)[RecentHexIds.Count] + &quot;位&quot;;&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        string output = $@&quot;/{channel} &lt;se.6&gt;&#xD;&#xA;{sepBold}&#xD;&#xA;♥ 蜂蜂小甜心  ☆  甜到你心中 ♥&#xD;&#xA;&quot;;&#xD;&#xA;        if (isSimp)     output += $&quot;♥ 从今天起～成为我的小工蜂 ♥\n♥ 欢迎{cnt}新舰长上舰～ ♥&quot;;&#xD;&#xA;        else            output += $&quot;♥ 从今天起～成為我的小工蜂 ♥\n♥ 歡迎{cnt}新艦長上艦～ ♥&quot;;&#xD;&#xA;        output += $&quot;\n☆ {string.Join(&quot; ☆\n☆ &quot;, RecentHexIds.Select(id =&gt; GetPlayerDesc(id)))} ☆&quot;;&#xD;&#xA;        RealPlugin.plug.InvokeNamedCallback(&quot;command&quot;, output);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    /// &lt;summary&gt; 从已解析的当前玩家中，根据 ID 生成描述，如 [职业] 名字。 &lt;/summary&gt;&#xD;&#xA;    string GetPlayerDesc(string hexId, string bkupName = null)&#xD;&#xA;    {&#xD;&#xA;        if (currentPlayers.TryGetValue(hexId, out var vd))&#xD;&#xA;        {&#xD;&#xA;            var name = $&quot;{vd.GetValue(&quot;name&quot;)}&quot;;&#xD;&#xA;            //if (vd.GetValue(&quot;id&quot;).ToString() == PlayerHexId) // 演示用&#xD;&#xA;            //    name = &quot;Argument Exception&quot;;&#xD;&#xA;            return $&quot;[{vd.GetValue(isSimp ? &quot;jobCN2&quot; : &quot;jobEN3&quot;)}] {name}&quot;;&#xD;&#xA;        }&#xD;&#xA;        else return bkupName ?? hexId; // 不应进入此分支&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    static string sepBold = &quot;━━━━━━━━━━━━━━━━━&quot;;&#xD;&#xA;    static string sepThin = &quot;─────────────────&quot;;&#xD;&#xA;&#xD;&#xA;    /// &lt;summary&gt; 生成当前排行并发送。 &lt;/summary&gt;&#xD;&#xA;    void OutputRanking()&#xD;&#xA;    {&#xD;&#xA;        var currentPlayerIds = new HashSet&lt;string&gt;(currentPlayers.Keys);&#xD;&#xA;        Ranking = ParseRecordedPlayersFromTable()&#xD;&#xA;            .Where(p =&gt; currentPlayerIds.Contains(p.HexId))&#xD;&#xA;            .OrderByDescending(p =&gt; p.Times)&#xD;&#xA;            .ThenBy(p =&gt; p.LastUpdated)&#xD;&#xA;            .ToList();&#xD;&#xA;        GenerateRanking();&#xD;&#xA;&#xD;&#xA;        /* /p &lt;se.6&gt;&#xD;&#xA;         ━━━━━━━━━━━━━━━━━&#xD;&#xA;         ☆ 蜂蜂小甜心 ☆ 小姐的 大♡航♡海～&#xD;&#xA;         ─────────────────&#xD;&#xA;         　　5 次　　Aaaaa Bbbbb&#xD;&#xA;         　　3 次　　Cccccccc Dddddddd&#xD;&#xA;         　　3 次　　阿阿阿&#xD;&#xA;         　　1 次　　Eeeeee&#xD;&#xA;         ━━━━━━━━━━━━━━━━━&#xD;&#xA;         */&#xD;&#xA;&#xD;&#xA;        string output = $&quot;/{channel} &lt;se.6&gt;\n\n{sepBold}\n☆ 蜂蜂小甜心 ☆  ♥  ☆ 大航海排行 ☆\n&quot;;&#xD;&#xA;        if (Ranking.Count() == 0) return;&#xD;&#xA;        output += string.Join(&quot;\n&quot;, Ranking.Select(p =&gt; p.ToRankingString(this))) + &quot;\n&quot; + sepBold;&#xD;&#xA;        RealPlugin.plug.InvokeNamedCallback(&quot;command&quot;, &quot;/e \n♡ 清空排行：/e bee reset&quot;);&#xD;&#xA;        RealPlugin.plug.InvokeNamedCallback(&quot;command&quot;, output);&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    /// &lt;summary&gt; 从永久表格中解析所有已记录的玩家数据。 &lt;/summary&gt;&#xD;&#xA;    IEnumerable&lt;Player&gt; ParseRecordedPlayersFromTable() &#xD;&#xA;        =&gt; GetTableVariable(true, &quot;HoneyBeeRanking&quot;)?.Rows&#xD;&#xA;            ?.Select(row =&gt; new Player(&#xD;&#xA;                hexId: row.Values[0],&#xD;&#xA;                name:  row.Values[1],&#xD;&#xA;                times: row.Values[2],&#xD;&#xA;                lastUpdated: row.Values[3]))&#xD;&#xA;            ?? Enumerable.Empty&lt;Player&gt;();&#xD;&#xA;&#xD;&#xA;    /// &lt;summary&gt; 计算排名，考虑并列 &lt;/summary&gt;&#xD;&#xA;    void GenerateRanking()&#xD;&#xA;    {&#xD;&#xA;        int rank = 1;&#xD;&#xA;        for (int i = 0; i &lt; Ranking.Count; i++)&#xD;&#xA;        {&#xD;&#xA;            Ranking[i].Rank = (i &gt;= 1 &amp;&amp; Ranking[i].Times == Ranking[i - 1].Times) // 当前的次数和前一个相同&#xD;&#xA;                ? Ranking[i - 1].Rank : rank;&#xD;&#xA;            rank++;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;&#xD;&#xA;    public class Player&#xD;&#xA;    {&#xD;&#xA;        public string HexId { get; set; }&#xD;&#xA;        public string Name { get; set; }&#xD;&#xA;        public int Times { get; set; }&#xD;&#xA;        public double LastUpdated { get; set; }&#xD;&#xA;        public int Rank { get; set; }&#xD;&#xA;&#xD;&#xA;        public Player(object hexId, object name, object times, object lastUpdated)&#xD;&#xA;        {&#xD;&#xA;            HexId = hexId.ToString();&#xD;&#xA;            Name = name.ToString();&#xD;&#xA;            Times = int.Parse(times.ToString(), System.Globalization.CultureInfo.InvariantCulture);&#xD;&#xA;            LastUpdated = double.Parse(lastUpdated.ToString(), System.Globalization.CultureInfo.InvariantCulture);&#xD;&#xA;        }&#xD;&#xA;&#xD;&#xA;        public string ToRankingString(HoneyBeeRanking instance)&#xD;&#xA;        {&#xD;&#xA;            string rank = Context.ToXIVChar(Rank.ToString(), true);&#xD;&#xA;            string times = Times.ToString();&#xD;&#xA;            times = new string(' ', (3 - times.Length) * 2) + times; // 左侧补全空格对齐&#xD;&#xA;            string player = instance.GetPlayerDesc(HexId, Name);&#xD;&#xA;            return $&quot;{rank}　{times} 次　　{player}&quot;;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}" ActionType="ExecuteScript" Description="Bee ❤ Bee ❤ Bee ❤ Bee ❤ Bee ❤ Bee ❤ Bee ❤ Bee ❤ Bee" DescBgColor="fac" DescTextColor="824" DescriptionOverride="True">
<Condition Enabled="true" Grouping="Or">
<ConditionSingle Enabled="true" ExpressionL="${ecallback:HoneyBeeRanking}" ExpressionTypeL="String" ExpressionR="0" ExpressionTypeR="String" ConditionType="StringEqualCase" />
</Condition>
</Action>
