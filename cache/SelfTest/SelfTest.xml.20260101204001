<Trigger Enabled="true" Id="a51cec9d-c8f1-4c7b-8d30-b87c7074eda2" Name="4.2 [C] 检查 ACT 插件顺序" RegularExpression="^SelfTest_PluginOrder:(?&lt;isManual&gt;[01])">
<Actions>
<Action OrderNumber="1" ActionType="ExecuteScript" ExecScriptExpression="using Advanced_Combat_Tracker;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;using System.Windows.Forms;&#xD;&#xA;using System.Linq;&#xD;&#xA;// 反射 API 限制，只能获取 Type 的名称，手动转换一下&#xD;&#xA;var typeToName = new Dictionary&lt;string, string&gt;&#xD;&#xA;{&#xD;&#xA;    { &quot;FFXIV_ACT_Plugin.FFXIV_ACT_Plugin&quot;, &quot;FFXIV_ACT_Plugin&quot; },&#xD;&#xA;    { &quot;RainbowMage.OverlayPlugin.PluginLoader&quot;, &quot;OverlayPlugin&quot; },&#xD;&#xA;    { &quot;PostNamazu.PostNamazu&quot;, &quot;PostNamazu&quot; },&#xD;&#xA;    { &quot;TriggernometryProxy.ProxyPlugin&quot;, &quot;Triggernometry&quot; },&#xD;&#xA;    { &quot;ACT.FoxTTS.FoxTTSPlugin&quot;, &quot;FoxTTS&quot; },&#xD;&#xA;    { &quot;Cafe.Matcha.MatchaInit&quot;, &quot;Matcha&quot; },&#xD;&#xA;    { &quot;ACT.FFXIVPing.FFXIVPingPlugin&quot;, &quot;FFXIVPing&quot; },&#xD;&#xA;    { &quot;Cactbot.PluginLoader&quot;, &quot;Cactbot&quot; },&#xD;&#xA;    { &quot;StarlightBreaker.MainClass&quot;, &quot;StarlightBreaker&quot; },&#xD;&#xA;    { &quot;AccurateWorldTravelQueue.PluginMain&quot;, &quot;AccurateWorldTravelQueue&quot; },&#xD;&#xA;    { &quot;ACT.TTS_CN.PluginMain&quot;, &quot;TTS_CN&quot; },&#xD;&#xA;    { &quot;SilverDasher.Loader.Loader&quot;, &quot;SilverDasher&quot; }&#xD;&#xA;};&#xD;&#xA;&#xD;&#xA;var pluginNames = ActGlobals.oFormActMain.ActPlugins&#xD;&#xA;    .Where(p =&gt; p.cbEnabled.Checked)&#xD;&#xA;    .Select(p =&gt; p.pluginObj.GetType().ToString())&#xD;&#xA;    .Select(typeName =&gt; typeToName.TryGetValue(typeName, out var name) ? name : typeName)&#xD;&#xA;    .Where(name =&gt; !name.ToLower().Contains(&quot;cafestore&quot;))&#xD;&#xA;    .ToList();&#xD;&#xA;&#xD;&#xA;List&lt;string&gt; errors = new List&lt;string&gt;();&#xD;&#xA;&#xD;&#xA;if (pluginNames[0] != &quot;FFXIV_ACT_Plugin&quot; ||&#xD;&#xA;    pluginNames[1] != &quot;OverlayPlugin&quot; ||&#xD;&#xA;    pluginNames[2] != &quot;Triggernometry&quot;)&#xD;&#xA;{&#xD;&#xA;    errors.Add(&quot;· 前三个插件按顺序应为：\n  1. FFXIV_ACT_Plugin\n  2. OverlayPlugin\n  3. Triggernometry&quot;);&#xD;&#xA;}&#xD;&#xA;var namazuIdx = pluginNames.IndexOf(&quot;PostNamazu&quot;);&#xD;&#xA;var cactbotIdx = pluginNames.IndexOf(&quot;Cactbot&quot;);&#xD;&#xA;var trigIdx = pluginNames.IndexOf(&quot;Triggernometry&quot;);&#xD;&#xA;var overlayIdx = pluginNames.IndexOf(&quot;OverlayPlugin&quot;);&#xD;&#xA;if (namazuIdx &gt;= 0 &amp;&amp; namazuIdx &lt; trigIdx)&#xD;&#xA;{&#xD;&#xA;    errors.Add(&quot;· 鲶鱼精邮差 PostNamazu 应置于触发器 Triggernometry 后面。&quot;);&#xD;&#xA;}&#xD;&#xA;if (cactbotIdx &gt;= 0 &amp;&amp; cactbotIdx &lt; overlayIdx)&#xD;&#xA;{&#xD;&#xA;    errors.Add(&quot;· Cactbot 应置于 OverlayPlugin 后面。&quot;);&#xD;&#xA;}&#xD;&#xA;if (errors.Any())&#xD;&#xA;{&#xD;&#xA;    var detail = &quot;${isManual}&quot; == &quot;1&quot; ? &quot;&quot; : &quot;\n（ACT 主界面 - 插件 - Triggernometry - 页面下方 远程触发器）&quot;;&#xD;&#xA;    MessageBox.Show(&#xD;&#xA;        &quot;ACT 插件顺序错误，插件不会正常工作：\n\n&quot; + string.Join(&quot;\n&quot;, errors) + &quot;\n\n你需要按远程触发器 - 自检工具箱中的说明调整。&quot; + detail + &quot;\n建议截图此弹窗以便调整。\n修复后需要重启 ACT。\n\n当前插件列表：\n· &quot; + string.Join(&quot;\n· &quot;, pluginNames),&#xD;&#xA;        &quot;Triggernometry 触发器自检：插件顺序&quot;, MessageBoxButtons.OK, MessageBoxIcon.Error);&#xD;&#xA;}&#xD;&#xA;else if (&quot;${isManual}&quot; == &quot;1&quot;)&#xD;&#xA;{&#xD;&#xA;    MessageBox.Show(&#xD;&#xA;        &quot;ACT 插件顺序正确。\n\n当前插件列表：\n· &quot; + string.Join(&quot;\n· &quot;, pluginNames),&#xD;&#xA;        &quot;触发器自检：插件顺序&quot;, MessageBoxButtons.OK, MessageBoxIcon.Information);&#xD;&#xA;}" />
</Actions>
</Trigger>
